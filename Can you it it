

import java.util.*;
import java.lang.*;
import java.io.*;

class main
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc = new Scanner(System.in);
		int t = sc.nextInt();
		while(t>0){
		    int a = sc.nextInt();
		    int b = sc.nextInt();
		    
		    int x = a/b;
		    if(a%b == 0){
		      System.out.println(x);
		    }else System.out.println("-1");
		    
		    t--;
		}
	}
}
